<!--pages/index/Component/Map/Map.wxml-->
<view class="viewTitle">
    <text class="view-Name">Map地图</text>
    <view class="lineView"></view>
</view>
<!--latitude:维度,longitude:经度-->
<view class="section section_gap">
      <map
        style="width: 100%; height: 500px;"
        latitude="{{latitude}}"
        longitude="{{longitude}}"
        markers="{{markers}}"
        covers="{{covers}}"
        >
      </map>
</view>
